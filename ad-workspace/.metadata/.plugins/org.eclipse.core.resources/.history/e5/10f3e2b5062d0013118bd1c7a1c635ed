/**
 * 
 */
package fbaue.ad.liste;

/**
 * @author florianbauer
 * 
 */
public interface AdListeInterface<T> {

	/**
	 * @param elem
	 */
	void cons(T elem);

	/**
	 * @return
	 */
	T head();

	/**
	 * @return
	 */
	int length();

	/**
	 * @return
	 */
	boolean isEmpty();

	/**
	 * @param elem
	 * @param position
	 */
	void insert(T elem, int position);

}
